{
    "name": "lisq/yii2-kafka",
    "description": "yii2 kafka 生产者、消费者组件",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","rdkafka","kafka","mq"],
    "license": "MIT",
    "authors": [
        {
            "name": "lisqorz",
            "email": "mail@lishunqi.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "yiisoft/yii2": "~2.0.0",
        "ext-rdkafka": "*"
    },
    "autoload": {
        "psr-4": {
            "lisq\\kafka\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "bootstrap": "lisq\\kafka\\DependencyInjection"
    }
}
